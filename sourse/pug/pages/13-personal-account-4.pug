block pagecontent


	+header()
		+top-nav-cabinet()
	+breadcrumb('Асосий саҳифа', 'Кабинет', title)
	+sPersonalAccount()
		.row.row--main
			.col-lg.col--aside
				+b.UL.navigation.d-none.d-lg-block
