mixin sChatCards-47(data)
	// start sChatCards
	+b.sChatCards.section#sChatCards&attributes(attributes) 
		//- +chartCardHor(`1`,`Ветиринар`,`Александра Алиева`,'m').mb-4

		+b.chat-card--horizontal.mb-4
			+e.row.row
				.col-auto
