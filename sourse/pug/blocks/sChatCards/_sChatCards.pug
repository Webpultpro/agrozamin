				.col
					.row
						
						.col-lg-3.col-md-4.mb-3
							.h3.text-primary.mb-3 Аграном
							.h6.mb-0 Собиров Алишер
							.text-secondary Сўнги ташриф: 22:30 
						.col-md
							.fw-500 Агрономнинг вазифалари қуйидагилардан иборат:
							ul 
								li Агрономия соҳасида илмий тадқиқотлар ўтказиш.
								li Екиш ва екиш учун тупроқни тайёрлаш ишларини ташкил етиш.
								li Тупроққа ўғит тайёрлаш ва киритиш бўйича чора-тадбирларни ишлаб чиқиш.
		+b.btn-w100(type="button") Мутаҳасис билан онлайн видео мулоқат ўрнатиш учун вақтни белгиланг
		+e.block
			+b.calendar-line
				+e.btn
					+icon('chevron-left')
				+e.data 4 - 10 Январь 2022
				+e.btn
					+icon('chevron-right')
			+b.week-line
				mixin d(t,n)
					+e.item(href="#")&attributes(attributes)  
						+e.title= t
						+e.date= n
				+d("Пн",'4').active
				+d("Вт",'5')
				+d("Ср",'6')
				+d("Чт",'7')
				+d("Пт",'8')
				+d("Сб",'9')
				+d("Вс",'10')
				+e.hr Мутаҳасиснинг бўш вақтлари
			+b.date-body
				
				mixin item(t, a, i)
					+e.line 
						.row
							+e.time.col= t
							+e.col.col-md-auto.col
