mixin chartCard(i,t,p, b)
	+b.chat-card 
		+e.row.row
			.col-auto
				+e.img-wrap.bg-wrap
					+icon('user')
					if i
						+pic(`avatar-${i}.png`,'bg',0).picture-bg
			.col
				.h6.mb-0= t
				.text-secondary= p
				+stars()
				if !b
					+e.footer
						+e.btn.btn-outline-primary(href="#") Кириш
					
mixin chartCardHor(i,t,p, empty)
	+b.chat-card--horizontal&attributes(attributes)
		+e.row.row.align-items-md-center
			.col-auto
				+e.img-wrap.bg-wrap
					+icon('user')
					if i
						+pic(`avatar-${i}.png`,'bg',0).picture-bg
			.col
				.row.align-items-md-center
					
					.col-md.mb-md-0.mb-3
						.h6.mb-0= t
						.text-secondary= p
						if empty
							.text-info.mt-2 Онлайн
					.col-md-auto
						if empty
							+e.btn.btn-outline-secondary2(href="#") 
								+icon("pen")
								| Таҳрирлаш
						else
							+e.btn.btn-primary(href="#") Батафсил
							
				//- +stars()
				//- +e.footer
mixin chartCardHor2(i,p, icon)
	+b.chat-card--horizonta
		+e.link-base(href="#")
		+e.row.row.align-items-md-center
			.col-auto.order-first
				+e.img-wrap.bg-wrap
					+icon('user')
					if i
						+pic(`avatar-${i}.png`,'bg',0).picture-bg
			.col
				.row.align-items-center.gx-2
					
					.col-lg-4.mb-3
						.h6 Султонова Мадина
						.alert.label&attributes(attributes)
							if icon
								+icon(icon)
							= p
					.col-auto
						.text-secondary Сана
						p 04.01.2021
					.col
						.text-secondary Вақти
						p 15:30-16:00
					.w-100.d-md-none
					block
					
							
				//- +stars()
				//- +e.footer

mixin sChatCards(data)
	// start sChatCards
	+b.sChatCards.section#sChatCards&attributes(attributes)
		+e.row.row
			+e.col.col-xl-4.col-md-6 
				+chartCard(`1`,`Ветиринар`,`Александра Алиева`)
			+e.col.col-xl-4.col-md-6 
				+chartCard(`2`,`Аграном`,`Собиров Алишер`)
			+e.col.col-xl-4.col-md-6 
				+chartCard(`3`,`Агротехник`,`Қаҳрамонов Рустам`)
			+e.col.col-xl-4.col-md-6 
				+chartCard(`1`,`Агротехник`,`Алимов Равшан`)
			+e.col.col-xl-4.col-md-6 
				+chartCard(``,`Зоотехник`,`Жураев Зокир`)
		+e.banner
			.row
				.col
					h2 Консултант бўлишни истайсизми?
					p Бизга қўшилинг ва мижозлар учун ҳизмат кўрсатинг
					+e.btn.btn-primary(href="#") Ўтиш
				.col-auto.d-none.d-lg-block
					+pic("chat-banner.png",'',0)

	// end sChatCards
