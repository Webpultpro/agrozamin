							p.h5.strong.mb-0!= p
				+item(1, `Чорвачилик`)(class="wow animate__fadeInUp" data-wow-delay=".2s")
				+item(2, `Паррандачилик`)(class="wow animate__fadeInUp" data-wow-delay=".3s")
				+item(3, `Сабзавот ва полизчилик`)(class="wow animate__fadeInUp" data-wow-delay=".4s")
				+item(4, `Иссиқхона хўжалиги`)(class="wow animate__fadeInUp" data-wow-delay=".5s")
				+item(5, `Узумчилик`)(class="wow animate__fadeInUp" data-wow-delay=".6s")
				+item(6, `Бизнес, молия ва ҳуқуқий саводхонлик `)(class="wow animate__fadeInUp" data-wow-delay=".7s")
	
	// end sOurDuty
mixin sOurDuty3(data)
	// start sOurDuty
	+b.SECTION.sOurDuty.section#sOurDuty
		.container
			+b.section-title.text-center(class="wow animate__fadeInUp" data-wow-delay=".1s")
				h2 Таълим йўналишлари
			.row
				mixin item(i, p)
					.col-md-6.col-lg-4
						+e.item--link(href="#")&attributes(attributes)
							//- +icon(`duty-${i}`)
