mixin sProvider(online)
	// start sProdBody
	+b.sProdBody.section#sProvider&attributes(attributes)
		.container
			+e.provider-region
				a(href="#" target="_blank")
					+e.provider
						.icon-wrapper
							+pic('agriculture-logo','','')
						+e.title-wrapper
							h3 OOO "Irrigation Solution"
							if online
								p.online Онлайн
									span
							if !online
								p Сўнги ташриф: &nbsp;
									span 22:30
				+cardlinksWithPhoneNoLike()
			+e.inner
				//- +info-message(`Сотув жараёнида махсулотнинг умумий миқдори ўзгарган тақдирда "Ўзгартириш киритиш" орқали махсулотнинг умумий миқдорини ўзгартиришингиз мумкин`).mb-4
				.row
					+e.col-right.col-md
						+e.caption
							h1 Компания ҳақида
							.text-body
								p OOO "Irrigation Solution" Ўзбекистонннинг қишлоқ хўжалиги корхоналари учун товарлар ва хизматларни ишлаб чиқариш учун вертикал равишда интеграцияланган корхона ҳисобланади. Сизга қишлоқ хўжалиги ҳайвонлар ва қушлар учун озуқа қўшимчаларини таклиф етамиз. Айни пайтда биз сут маҳсулдорлигини ошириш ва сигирларнинг gynecologic ва касалликларини олдини олиш учун ўз ишлаб чиқаришимизда енергия комплексларини ишлаб чиқарамиз.
							+infoTable()
					+e.trust-seller-col
						+trustedSeller()
						.registration-date Рўйхатга олинган сана: &nbsp;
							span 10.06.2022
	// end sProvider